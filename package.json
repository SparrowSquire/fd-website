{
  "name": "fluid-designs",
  "version": "1.1.0",
  "description": "Claire Squires' portfolio",
  "main": "index.js",
  "scripts": {
    "build": "npm run sass && npm run webpack",
    "jest:ci": "jest --json --outputFile=test/results/jest-test-results.json test/components/",
    "sass": "node-sass ./sass/index.scss --output-style compressed -o ./public/styles",
    "start": "webpack-dev-server --config ./webpack.config.js --mode development",
    "test": "npm run jest:ci",
    "test:coverage": "jest --coverage --colors",
    "webpack": "webpack --config ./webpack.config.js"
  },
  "author": "Claire Squire",
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^24.9.0",
    "babel-loader": "^7.1.5",
    "babel-plugin-dynamic-import-node": "^2.3.0",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-syntax-object-rest-spread": "^6.13.0",
    "babel-plugin-transform-builtin-classes": "^0.6.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-es2015-classes": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-runtime": "^6.26.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "node-sass": "^4.13.0",
    "react-test-renderer": "^16.11.0",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "dependencies": {
    "babel-cli": "^6.26.0",
    "react": "^16.10.2",
    "react-dom": "^16.10.2"
  }
}
